import java.util.Scanner;
public class Operator{
    public static double getResult(double numberA,double numberB,String operator){

        double result = 0;
        switch(operator){
            case "+":
                result = numberA + numberB;
                break;
            case "-":
                result = numberA - numberB;
                break;
            case "*":
                result = numberA * numberB;
                break;
            case "/":
                result = numberA / numberB;
                break;
            default:
                System.out.println("暂不支持此运算符!!");
        }
        return result;
    }
}
