public class Proxy implements IGiveGift{
    Pursuit pursuit;
    MM mm;
    public Proxy(Pursuit pursuit, MM mm){
        this.pursuit = pursuit;
        this.mm = mm;
    }
    public void giveDolls(){
        System.out.println(mm.getName() + "你好,\n 我是代理\n" + pursuit.getName() + "说: ");
        pursuit.giveDolls();
    }
    public void giveFlowers(){
        System.out.println(mm.getName() + "你好,\n 我是代理\n" + pursuit.getName() + "说: ");
        pursuit.giveFlowers();
    }
}
